{
  "name": "<%= packageName %>",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "build": "npm run gen-odata && urb build",
    "deploy": "ui5-deployer deploy",
    "gen-odata": "odata2model -s src/asset/odata/odata-service.xml -o src/odata/odata-service --service-name MainODataService",
    "start": "urb start",
    "test": "urb test"
  },
  "dependencies": {
    "@cpro-js/history-ui5": "^4.0.0",
    "@cpro-js/react-core": "^0.3.0",
    "@cpro-js/react-ui5-notification": "^0.1.1",
    "@cpro-js/react-ui5-webcomponents-form": "^0.27.2",
    "@odata2ts/axios-odata-client": "^0.2.2",
    "@odata2ts/odata-service": "^0.7.1",
    "@ui5/webcomponents": "^1.5.0",
    "@ui5/webcomponents-fiori": "^1.5.0",
    "@ui5/webcomponents-react": "^0.26.1",
    "query-string": "^7.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-jss": "^10.9.0",
    "react-router": "5.3.3",
    "react-router-dom": "5.3.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@cpro-js/ui5-react-build": "*",
    "@odata2ts/odata2model": "^0.13.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack-env": "^1.17.0",
    "husky": "^8.0.1",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "prettier-plugin-import-sort": "0.0.7",
    "prettier-plugin-package": "^1.3.0",
    "typescript": "^4.6.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  }
}

{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "sourceMap": false,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "temp-templates", "src/app/templates", "generators"]
}
